<Window x:Class="Zelegram.Views.KeyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:local="clr-namespace:Zelegram.Views"
        mc:Ignorable="d"
        Title="KeyWindow" Height="200" Width="250">
    <StackPanel>
        <TextBox Margin="20" Name="KeyTextBox" FontSize="15" materialDesign:HintAssist.Hint="Enter Your Key : " Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
        <TextBlock Name="Error" Visibility="Hidden" Foreground="Red" HorizontalAlignment="Center">Entered Wrong Key</TextBlock>
        <Button Margin="10" Click="Button_Click">Sign Up</Button>
    </StackPanel>
</Window>
